{
    "manifest_version": 3,
    "name": "unBlob",
    "version": "1.0",
    "description": "Best unblob Tool. Open any audio/video outside of websites! ðŸ‘Œ",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "all_frames": true,
        "css": [
            "content-styles.css"
        ],
        "js": [
            "content-script.js"
        ],
        "run_at": "document_start"
    }],
    "web_accessible_resources": [{
        "resources": ["website-script.js"],
        "matches": ["<all_urls>"]
    }]
}